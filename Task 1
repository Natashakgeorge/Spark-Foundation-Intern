library(readxl)
library(ggplot2)
study_csv <- read_excel("C:/Users/HP/OneDrive/Desktop/project/study.csv.xlsx")
> View(study_csv)
model<- lm(Scores~Hours,study_csv)
model
ggplot(study_csv,aes(x=Hours,y=Scores))+geom_point()
plot(data)
abline(model, col="yellow")
a<-study_csv.frame(Hours=9.25)
result<-predict(model,a)
print(result)
